{
    "concurrency": 4,
    "enableGC": true,
    "vendor": true,
    "vendoredLinters": true,
    "deadline": "600s",
    "minConfidence": 0.0,
    "lineLength": 160,
    "test": true,
    "sort": ["path"],
    "enable": [
        "deadcode",
        "errcheck",
        "goconst",
        "golint",
        "ineffassign",
        "interfacer",
        "lll",
        "maligned",
        "megacheck",
        "misspell",
        "structcheck",
        "unconvert",
        "varcheck",
        "vet",
        "vetshadow"
    ],
    "severity": {
        "deadcode": "error",
        "errcheck": "error",
        "goconst": "error",
        "golint": "error",
        "ineffassign": "error",
        "interfacer": "error",
        "lll": "error",
        "maligned": "error",
        "megacheck": "error",
        "misspell": "error",
        "structcheck": "error",
        "unconvert": "error",
        "varcheck": "error",
        "vet": "error",
        "vetshadow": "error"
    },
    "skip": [
        "third_party"
    ],
    "exclude": [
        ".pb.go"
    ]
}